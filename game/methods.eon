method:
  name: vignette
  vertex: shaders/vignette.vs
  fragment: shaders/vignette.fs
  attributes:
    position
  uniforms:
    gWVP: mat4
    gColorMap: sampler
    gScreenSize: vec2
    gProperties: vec3

method:
  name: "bounding box"
  vertex: shaders/bbox.vs
  fragment: shaders/bbox.fs
  attributes:
    position
  fragdata:
    diffuseOut
  uniforms:
    gWVP: mat4
    gColor: vec3
